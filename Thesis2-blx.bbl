    \verb 10.1111/j.1365-2664.2011.02048.x
    \endverb
    \field{isbn}{0021-8901}
    \field{issn}{00218901}
    \field{title}{{Beyond species: Functional diversity and the maintenance of
  ecological processes and services}}
    \field{journaltitle}{Journal of Applied Ecology}
    \field{year}{2011}
  \endentry

