    \endverb
    \field{isbn}{0169-5347}
    \field{issn}{01695347}
    \field{title}{{Why intraspecific trait variation matters in community
  ecology}}
    \field{journaltitle}{Trends in Ecology and Evolution}
    \field{year}{2011}
  \endentry

