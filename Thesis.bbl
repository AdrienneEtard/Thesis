    \verb 10.1002/ece3.2579
    \endverb
    \field{isbn}{20457758}
    \field{issn}{20457758}
    \field{title}{{The database of the PREDICTS (Projecting Responses of
  Ecological Diversity In Changing Terrestrial Systems) project}}
    \field{journaltitle}{Ecology and Evolution}
    \field{year}{2017}
  \endentry

